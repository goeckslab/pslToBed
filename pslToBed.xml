<tool id="pslToBed" name="pslToBed" version="0.1.0">
    <requirements>
        <!-- Requires Conda -->
        <requirement type="package" version="324">ucsc-psltobed</requirement>
    </requirements>
    <stdio>
        <exit_code range="1:" />
    </stdio>
    <command><![CDATA[
        pslToBed $input $output
    ]]></command>
    <inputs>
        <param name="input" type="data" format="psl" />
    </inputs>
    <outputs>
        <data name="output" format="bed" />
    </outputs>
    <help><![CDATA[
        pslToBed: Transform a psl format file to a bed format file.
    ]]></help>
</tool>