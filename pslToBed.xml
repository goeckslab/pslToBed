<tool id="pslToBed" name="pslToBed" version="0.1.0">
    <requirements>
    </requirements>
    <stdio>
        <exit_code range="1:" />
    </stdio>
    <command><![CDATA[
        chmod +x $__tool_directory__/pslToBed
        $__tool_directory__/pslToBed $input $output
    ]]></command>
    <inputs>
        <param name="input" type="data" format="psl" />
    </inputs>
    <outputs>
        <data name="output" format="bed" />
    </outputs>
    <help><![CDATA[
        pslToBed: transform a psl format file to a bed format file.
    ]]></help>
</tool>